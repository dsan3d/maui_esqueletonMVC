<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="maui_esqueletonMVC.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:maui_esqueletonMVC"
    Shell.FlyoutBehavior="Flyout">
    <FlyoutItem Title="Inicio" Icon="myicono.png">
        <Tab>
            <ShellContent  Title="Inicio"
            ContentTemplate="{DataTemplate local:MainPage}" Route="MainPage" />
        </Tab>
    </FlyoutItem>

</Shell>
